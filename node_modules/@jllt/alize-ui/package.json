    "postinstall": "node scripts/copy-fonts.mjs",
    "dev": "next dev --turbopack",
    "dev:devtools": "NEXT_PUBLIC_ALIZE_DEVTOOLS=true next dev --turbopack",
    "dev:webpack": "next dev --webpack",
    "build": "npm run build:components && npm run build:css && npm run build:copy && next build",
    "build:lib": "npm run build:components && npm run build:css && npm run build:copy",
    "build:components": "tsup",
    "build:css": "npx tailwindcss -i src/styles/alize.css -o dist/alize.css --minify",
    "build:copy": "node scripts/build.mjs",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:mutation": "npx stryker run",
    "security:audit": "bash scripts/security-audit.sh",
    "check": "npm run lint && npm run format:check && npm run type-check",
